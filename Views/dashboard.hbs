<script>
  const calendarContainer = document.querySelector(".calendar-container");
    const currentMonthElement = document.querySelector(".current-month");
    const prevBtn = document.querySelector(".prev-btn");
    const nextBtn = document.querySelector(".next-btn");

    let currentDate = new Date();
    let currentMonth = currentDate.getMonth();
    let currentYear = currentDate.getFullYear();

    function generateCalendar() {
      const firstDayOfMonth = new Date(currentYear, currentMonth, 1);
      const lastDayOfMonth = new Date(currentYear, currentMonth + 1, 0);
      const prevMonthDays = firstDayOfMonth.getDay();
      const totalDays = lastDayOfMonth.getDate();

      const daysOfWeek = ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'];

      let calendarHTML = `<div class="calendar-header">
                            <span class="prev-btn">&lt;</span>
                            <span class="current-month">${getMonthName(currentMonth)} ${currentYear}</span>
                            <span class="next-btn">&gt;</span>
                          </div>
                          <div class="calendar-grid">`;

      for (const day of daysOfWeek) {
        calendarHTML += `<div class="weekday">${day}</div>`;
      }

      let dayCounter = 1;
      for (let i = 0; i < 42; i++) {
        if (i >= prevMonthDays && dayCounter <= totalDays) {
          calendarHTML += `<div class="calendar-day">${dayCounter}</div>`;
          dayCounter++;
        } else {
          calendarHTML += `<div class="calendar-day"></div>`;
        }
      }

      calendarHTML += `</div>`;
      calendarContainer.innerHTML = calendarHTML;

      const calendarDays = document.querySelectorAll(".calendar-day");
      calendarDays.forEach((day) => {
        day.addEventListener("click", () => {
          // Handle day click event here
          // You can implement custom behavior when a day is clicked
        });
      });
    }

    function getMonthName(monthIndex) {
      const months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
      return months[monthIndex];
    }

    prevBtn.addEventListener("click", () => {
      currentMonth--;
      if (currentMonth < 0) {
        currentMonth = 11;
        currentYear--;
      }
      generateCalendar();
    });

    nextBtn.addEventListener("click", () => {
      currentMonth++;
      if (currentMonth > 11) {
        currentMonth = 0;
        currentYear++;
      }
      generateCalendar();
    });

    generateCalendar();
</script>

